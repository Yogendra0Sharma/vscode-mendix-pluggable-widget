{
	"name": "vscode-mendix-pluggable-widget",
	"displayName": "vscode-mendix-pluggable-widget",
	"description": "Mendix pluggable widget generator extension",
	"version": "1.1.0",
	"icon": "img/icon.png",
	"publisher": "yogendrasharma",
	"repository": "https://github.com/Yogendra0Sharma/vscode-mendix-client-api-docs",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.installNodeJS",
		"onCommand:extension.installGenerator",
		"onCommand:extension.createWidget"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.installNodeJS",
				"category": "Mendix",
				"title": "Install NodeJS"
			},
			{
				"command": "extension.installGenerator",
				"category": "Mendix",
				"title": "Install Pluggable Widget Generator"
			},
			{
				"command": "extension.createWidget",
				"category": "Mendix",
				"title": "Create Widget"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
